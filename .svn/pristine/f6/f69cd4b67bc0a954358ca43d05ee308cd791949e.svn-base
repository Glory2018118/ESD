/*variable*/
:root {
    --myTextCol: #12d269;
}

/*for text*/

.myText{
    color :  var(--myTextCol);
}

.signUpButton{
    background : var(--myTextCol);
    color : white;
}

.textSize16{
    font-size : 16px;
}
/*for navvar*/


.myNavBg{
    background-color: white;

}

.navHeadItem{
    color : var(--myTextCol);
    font-size : 16px;
    font-family: Constantia;
    padding: 2px;
}

nav li:hover{
    text-decoration: underline;

}

nav a:hover{
    color : var(--myTextCol);
}

nav > ul > li > div > .dropdown-item:hover{
    color : var(--myTextCol);
    text-decoration: underline;
}

nav > ul > li > div > a:hover{
    text-decoration: underline !important;
}



